<h1>Listing users</h1>
<table>
<tr>
<th>Name</th>
<th></th>
<th></th>
<th></th>
</tr>
<% @users.each do |user| %>
<tr>
<td><%= user.name %></td>
<td><%= link_to 'Show', user %></td>
<td><%= link_to 'Edit', edit_user_path(user) %></td>
<td><%= button_to "delete", user, :method=>:delete, :class=>:destroy %></td>
</tr>
<% end %>
</table>
<br />
<%= link_to 'New User', new_user_path %>